{
  "name": "awa",
  "version": "0.1.0",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.0",
    "config": "^3.2.4",
    "express": "^4.17.1",
